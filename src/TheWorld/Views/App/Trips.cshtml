@{
    ViewBag.Title = "Trips Page";
}
@section Scripts{
    <script src="~/lib/underscore/underscore-min.js"></script>
    <script src="~/lib/angular/angular.min.js"></script>
    <script src="~/lib/angular-route/angular-route.min.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAIv6HoPPj9Mc7HSNpRWzoMgPavSX5e_-c"></script>
    <script src="~/lib/gmaps/gmaps.min.js"></script>
    <script src="~/lib/travelmap/travelmap.min.js"></script>


    <environment names="Development">
        <script src="~/js/simpleControl.js"></script>
        <script src="~/js/app-trips.js"></script>
        <script src="~/js/tripsController.js"></script>
        <script src="~/js/tripsEditorController.js"></script>
    </environment>
    <environment names="Production,Staging">
        <script type="text/javascript" src="~/lib/_app/Site.js"></script>
        <script type="text/javascript" src="~/lib/_app/simpleControl.js"></script>
        <script type="text/javascript" src="~/lib/_app/app-trips.js"></script>
        <script type="text/javascript" src="~/lib/_app/tripsController.js"></script>
        <script type="text/javascript" src="~/lib/_app/tripsEditorController.js"></script>
    </environment>
}
<div class="row" ng-app="app-trips">
    <div ng-view></div>


</div>